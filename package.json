  "scripts": {
    "heroku-prebuild": "npm prune",
    "heroku-postbuild": "npm rebuild node-sass && typings install && tsc && ng build --environment=\"$NODE_ENV\" --aot",
    "start": "node server/index",
    "ng": "ng",
    "build": "typings install && tsc",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "tsc && concurrently \"nodemon ./server/index --watch server\" \"gulp\" \"ng serve --proxy-config proxy.conf.json\"",
    "server": "tsc && node server/index",
    "client": "ng serve --proxy-config proxy.conf.json",
    "https": "ng serve --ssl 1 --ssl-key \"./server/cert/server.key\" --ssl-cert \"./server/cert/server.crt\" --proxy-config proxy.conf.json"
  },
